<%- include('header', { title: 'Zakończone przetargi' }) %>

<h1>Lista zakończonych przetargów</h1>
<table>
  <thead>
    <tr>
      <th>#</th>
      <th>Tytuł</th>
      <th>Status</th>
    </tr>
  </thead>
  <tbody>
    <% tenders.forEach((t, i) => { %>
      <tr>
        <td><%= i + 1 %></td>
        <td><a href="/tenders/<%= t.id %>"><%= t.title %></a></td>
        <td>Zakończono</td>
      </tr>
    <% }) %>
  </tbody>
</table>

<%- include('footer') %>
